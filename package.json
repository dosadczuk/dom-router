{
  "name": "dom-router",
  "description": "Very basic, zero configuration router for single HTML file websites.",
  "version": "0.2.0",
  "private": true,
  "author": {
    "name": "Damian Osadczuk",
    "url": "https://github.com/dosadczuk"
  },
  "homepage": "https://github.com/dosadczuk/dom-router",
  "keywords": [
    "router",
    "dom"
  ],
  "files": [
    "build"
  ],
  "main": "./build/dom-router.umd.js",
  "module": "./build/dom-router.es.js",
  "exports": {
    ".": {
      "import": "./build/dom-router.es.js",
      "require": "./build/dom-router.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "regexparam": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "jsdom": "^19.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vitest": "^0.7.12"
  }
}
